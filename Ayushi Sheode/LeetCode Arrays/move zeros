class Solution {
public:
    void moveZeroes(vector<int>& nums) {
        
        int a=0;
        
        
        
        for(int i=0;i<=nums.size()-1;i++)
        {
            if(nums[i]!=0) 
            {
                nums[a]=nums[i];
                a++;
            }
        }
        for(int i=a;i<=nums.size()-1;i++)
        {
            nums[i]=0;
        }
        for(int i=0;i<=nums.size()-1;i++)
        {
            cout<<nums[i]<<" ";
        }
    }
};

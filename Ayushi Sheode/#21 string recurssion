#include <bits/stdc++.h>

using namespace std;

void reverseString(string &s,int i,int j)
{
    if(i<j)
    {
        swap(s[i],s[j]);
        i++;
        j--;
        reverseString(s,i,j);
    }
}



int main()
{
    string s;

    getline(cin,s);
    reverseString(s,0,s.size()-1);

    cout<<s;
    return 0;
}

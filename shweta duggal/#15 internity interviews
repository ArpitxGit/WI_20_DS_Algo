#include<bits/stdc++.h>
using namespace std;

int main()
{
    int n;
    cin>>n;
    string candidate[n];
    int i;
    for(i=0;i<n;i++)
    {
        cin>>candidate[i];
    }
    int sem[n];

    for(i=0;i<n;i++)
    {
        cin>>sem[i];
    }

    string intervier[2];
    for(i=0;i<2;i++)
    {
        cin>>interviewer[i];
    }

    string even_name[n/2+1],odd_name[n/2];

    int even_sem[n/2+1],odd_sem[n/2];

    int a=0,b=0,c=0,d=0;

    for(i=0;i<n;i++)
    {
        if(i%2==0)
        {
            even_name[a]=candidate[i];
            a++;
            even_sem[c]=sem[i];
            c++;
        }
        else
        {
            odd_name[b]=candidate[i];
            b++;
            odd_sem[d]=sem[i];
            d++;
        }
    }

    if(n%2==0)
    {
        for(i=0;i<a;i++)
        {
            cout<<even_name[i]<<" ";
        }
        cout<<endl;
        for(i=0;i<b;i++)
        {
            cout<<odd_name[i]<<" ";
        }
    }
    else
    {
        int min1,min2;
        min1=even_sem[0];
        min2=odd_sem[0];
        for(i=0;i<c;i++)
        {
            if(even_sem[i]<min1)
            {
                min1=even_sem[i];
            }
        }
        for(i=0;i<d;i++)
        {
            if(odd_sem[i]<min2)
            {
                min2=odd_sem[i];
            }
        }
        cout<<endl;
        if(min1<=min2)
        {
            for(i=0;i<a;i++)
            {
                cout<<even_name[i]<<" ";
            }
            cout<<endl;
            for(i=0;i<b;i++)
            {
                cout<<odd_name[i]<<" ";
            }
        }
        else
        {
            for(i=0;i<b;i++)
            {
                cout<<odd_name[i]<<" ";
            }
            cout<<endl;
            for(i=0;i<a;i++)
            {
                cout<<even_name[i]<<" ";
            }
        }
    }

    return 0;
}

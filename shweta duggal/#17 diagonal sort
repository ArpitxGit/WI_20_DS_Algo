  
vector<vector<int>> solve(vector<vector<int>>& matrix) {
    int rows=matrix.size();
    int cols=matrix[0].size();

    while(rows>1)
    {
        for(int i=0;i<rows-1;i++)
        {
            for(int j=0;j<cols-1;j++)
            {
                if(matrix[i][j]>matrix[i+1][j+1])
                   swap(matrix[i][j],matrix[i+1][j+1]);
            }
        }

        rows--;
    }

    return matrix;
}

2
include <iostream.h>
using namespace std;
int main()
{

int N;
cin>>N;
int arr[N];
for(int i=0;i<N;i++)
{
cin>>arr[i];
}
int a,b,c;
for(int i = 0; i < N-2; i++)
{
for(int j=i+1;j <N-1; j++)
{
for(int k =i+2; k<N;k++)
{
a = arr[i];
b = arr[j];
c = arr[k];
if(aa + bb == c*c)
cout << a <<" "<<b<<" "<<c<<endl;

}
}
}
return 0;
}

#include <iostream>
#include <bits/stdc++.h>
using namespace std;

int main()
{
    int n,x=0;
    cout<<"Enter the number of caps (number should be odd): ";
    cin>>n;
    int caps[n];
    cout<<"Enter the colours of caps as a number: ";
    for(int i=0;i<n;i++)
    {
       cin>>caps[i]; 
    }
    for(int i=0;i<n;i++)
    {
        x=x^caps[i];
    }
    cout<<"Unique cap is: "<<x;
    return 0;
}

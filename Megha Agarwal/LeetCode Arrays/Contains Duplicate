class Solution {
public:
    bool containsDuplicate(vector<int>& nums) 
    { int count=0;
     int n= nums[0];
      for(int i=1;i<=nums.size()-1;i++)
      {
        if(n==nums[i])
        {
            count++;
        }
      }
     if(count==1)
     {
         return true;
     }
     else
     {
         return false;
     }
      
        
    }
};

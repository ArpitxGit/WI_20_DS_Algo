#include<iostream>
#using namespace std;
int max(int x,int y,int z)
{
if(x>y && x>z)
{
return x;
}
if(y>x && y>z)
{
return y;
}
if(z>y && z>x)
{
return z;
}
}
bool check(int x,int y,int z)
{
int a,b,c;
a=max(x,y,z);
if(a==x)
{
b=y;
c=z;
}
else if(a==z)
{
b=y;
c=x;
}
else
{
a=y;
b=x;
c=z;
}
if((aa) ==(bb)+(c*c)){return true;}
else
return false;

}

int main()
{
int i,j,k,n,arr[100];
cout<<"Enter no of Elements:";
cin>>n;
for(i=0;i<n;i++)
{
cin>>arr[i];
}
for(i=0;i<n;i++)
for(j=i+1;j<n;j++)
for(k=j+1;k<n;k++)
{ if(check(arr[i],arr[j],arr[k]))
{
cout<<"Pythagorean triplet Possible";
if(false)
cout<<"Not possible";

              }
        }
}
